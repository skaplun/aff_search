extends layout

block content
    h1 enter keyword, tld in google you'd like to search, and your brand name
    form(id="keySearch" name="keySearch", action="/", method="post")
        input(type="text", name="keyword", placeholder="keyword", class="form-control")
        input(type="text", name="tld", placeholder="tld", class="form-control")
        input(type="text", name="brand",placeholder="brand", class="form-control")
        input(type="submit", class="btn btn-default")
    div(class="panel panel-default")
        div(class="panel-heading")
            h3(class="panel-title") Your results
        div(class="panel-body")
            div(id="spinner")
             table(class="table", id="results")
       
        
            
            

